<h1> VendorSweet </h1>
<%= form_for @vendor_sweets do |f| %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :sweet_id %>
  <%= f.collection_select :sweet_id, @vendor_sweets, :id , :name %>


  <%= f.label :vendor_id %>
  <%= f.collection_select :vendor_id, @vendor_sweets, :id, :name %>

  <%= f.submit %>

<% end %>
